            ecoPlastNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('')
        });
    }
    selectFilm(event) {
        this.selectedFilm = event.target.value;
    }
    exit() {
        this.formFilm.reset();
        this.filmService.displayDialogFilm = false;
    }
    onAddNewFilm() {
        this.formFilm.value.status = this.formFilm.value.status.value;
        this.filmService.addOklina(this.formFilm.value).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessAdd("okleiny");
                this.addedFilm.emit();
                this.exit();
            },
            error: (err) => {
                this.infoPopup.showErrorAdd("okleiny");
            },

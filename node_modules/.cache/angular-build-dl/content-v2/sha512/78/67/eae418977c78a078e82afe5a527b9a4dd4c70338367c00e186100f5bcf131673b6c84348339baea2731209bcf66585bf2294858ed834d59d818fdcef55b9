        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA },
        ];
    }
    ngOnInit() {
        this.formFilm = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormGroup"]({
            number: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required], asyncValidators: [src_app_validators_AppValidators__WEBPACK_IMPORTED_MODULE_4__["AppValidators"].filmExists], updateOn: 'blur' }),

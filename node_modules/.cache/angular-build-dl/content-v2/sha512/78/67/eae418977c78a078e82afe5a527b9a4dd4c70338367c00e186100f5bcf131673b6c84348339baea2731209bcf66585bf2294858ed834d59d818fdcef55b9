let FilmMainComponent = class FilmMainComponent {
    constructor(filmService, infoPopup) {
        this.filmService = filmService;
        this.infoPopup = infoPopup;
        this.film = [];
        this.producer = [];
        this.positionpersite = 10;
        this.onErr = {
            id: 0,
            number: "---",
            name: "---",
            producer: "---",
            ecoPlastNumber: "---",
            ralNumber: "---",
            imgLink: "---",
            status: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA
        };
        this.clonedFilms = {};
        this.clonedProducers = {};
    }
    ngOnInit() {
        this.err = false;
        this.loadData();
        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA },
        ];
    }
    loadData() {
        this.filmService.loadProducersSelect();
        this.filmService.getOklieny().subscribe({
            next: (res) => {
                this.film = res;
                this.err = false;
            },
            error: (err) => {
                this.infoPopup.showErrorDownload("okleiny");
                this.err = true;
                this.film.push(this.onErr);
            },
            complete: () => {
            }
        });
        this.filmService.getCountOkleina().subscribe({
            next: (res) => {
                this.filmCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("oklein");
            },
            complete: () => {
            }
        });
        this.filmService.getCountProducer().subscribe({
            next: (res) => {
                this.producerCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("producentów");
            },
            complete: () => {
            }
        });
    }
    showDialogToAddFilm() {
        this.filmService.displayDialogFilm = true;
    }
    showDialogToAddProducer() {
        this.filmService.displayDialogProducer = true;
    }
    onRowEditInit(film) {
        this.clonedFilms[film.id] = Object.assign({}, film);
    }
    onRowEditSave(film) {
        console.log(film);
        this.filmService.updateOkleina(film.id, film).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("okleiny");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("okleiny");
            },
            complete: () => {
                delete this.clonedFilms[film.id];
                this.loadData();
            }
        });
        // if (film.numerOkleiny > 0) {
        //     delete this.clonedFilms[film.numerOkleiny];
        //    // this.messageService.add({severity:'success', summary: 'Success', detail:'Car is updated'});
        // }
        // else {
        //     //this.messageService.add({severity:'error', summary: 'Error', detail:'Year is required'});
        // }
    }
    onRowEditCancel(film, index) {
        this.film[index] = this.clonedFilms[film.id];
        delete this.clonedFilms[film.id];
    }
    onRowEditInitProducer(producer) {
        this.clonedProducers[producer.id] = Object.assign({}, producer);
    }
    onRowEditProducer(producer) {
        this.filmService.updateProducer(producer.id, producer).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("producenta");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("producenta");
            },
            complete: () => {
                delete this.clonedProducers[producer.id];
                this.loadData();
            }
        });
    }
    onRowEditCancelProducer(producer, index) {
        this.producer[index] = this.clonedFilms[producer.id];
        delete this.clonedProducers[producer.id];
    }
};

            message: 'Na pewno chcesz usunąć tą pozycję?',
            accept: function accept() {
              //Actual logic to perform a confirmation
              _this4.clientService.deleteClient(client.id).subscribe({
                next: function next(res) {
                  _this4.infoPopup.showSuccessDelete("klienta");
                },
                error: function error(err) {
                  _this4.infoPopup.showErrorDelete("klient");
                },
                complete: function complete() {}
              });
            }
          });
        }
      }, {

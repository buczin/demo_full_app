/***/ "./src/app/catalog/catalog-main/catalog-main.component.ts":
/*!****************************************************************!*\
  !*** ./src/app/catalog/catalog-main/catalog-main.component.ts ***!
  \****************************************************************/
/*! exports provided: CatalogMainComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CatalogMainComponent", function() { return CatalogMainComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm2015/core.js");
/* harmony import */ var _catalog_service_catalog_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ../catalog-service/catalog.service */ "./src/app/catalog/catalog-service/catalog.service.ts");
/* harmony import */ var primeng_api__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! primeng/api */ "./node_modules/primeng/fesm2015/primeng-api.js");
/* harmony import */ var src_app_info_popup_info_popup_component__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! src/app/info-popup/info-popup.component */ "./src/app/info-popup/info-popup.component.ts");
/* harmony import */ var src_app_services_auth_service__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! src/app/_services/auth.service */ "./src/app/_services/auth.service.ts");
/* harmony import */ var _system_add_form_system_add_form_component__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ../system-add-form/system-add-form.component */ "./src/app/catalog/system-add-form/system-add-form.component.ts");
/* harmony import */ var _catalog_add_form_catalog_add_form_component__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! ../catalog-add-form/catalog-add-form.component */ "./src/app/catalog/catalog-add-form/catalog-add-form.component.ts");
/* harmony import */ var _angular_material_dialog__WEBPACK_IMPORTED_MODULE_8__ = __webpack_require__(/*! @angular/material/dialog */ "./node_modules/@angular/material/esm2015/dialog.js");









let CatalogMainComponent = class CatalogMainComponent {
    constructor(dialog, catalogService, infoPopup, auth, confirmationService) {
        this.dialog = dialog;
        this.catalogService = catalogService;
        this.infoPopup = infoPopup;
        this.auth = auth;
        this.confirmationService = confirmationService;
        this.catalog = [];
        this.system = [];
        this.loadingCatalog = false;
        this.onErrSystem = {
            id: 0,
            name: "---"
        };
        this.onErr = {
            id: 0,
            systemProfil: this.onErrSystem,
            number: "---",
            dimOuter: "---",
            dimInner: "---",
            dimOneSide: "---",
            dimTwoSide: "---",
            dimTwoSideInner: "---",
            dimTwoSideOuter: "---",
            actualMeasurement: false,
            hard: false,
            profileType: "---",
            comments: "---"
        };
        this.clonedCatalogs = {};
        this.clonedSystem = {};
    }
    ngOnInit() {
        this.err = false;
        this.loadData();
    }
    loadData() {
        this.loadingCatalog = true;
        this.catalogService.loadSystems();
        this.catalogService.getCatalogs().subscribe({
            next: (res) => {
                this.catalog = res;
                this.err = false;
            },
            error: (err) => {
                this.infoPopup.showErrorDownload("katalogu");
                this.err = true;
                this.catalog.push(this.onErr);
            },
            complete: () => {
                this.loadingCatalog = false;
            }
        });
        this.catalogService.getCountCatalog().subscribe({
            next: (res) => {
                this.catalogCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("katalogu");
            },
            complete: () => {
            }
        });
        this.catalogService.getCountSystem().subscribe({
            next: (res) => {
                this.systemCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("systemów");
            },
            complete: () => {
            }
        });
    }
    loadSelectedSystem(kat) {
        this.catalog = [];
        this.loadingCatalog = true;
        if (kat == -1) {
            this.catalogService.getCatalogs().subscribe({
                next: (res) => {
                    this.catalog = res;
                    this.err = false;
                },
                error: (err) => {
                    this.infoPopup.showErrorDownload("katalogu");
                    this.err = true;
                    this.catalog.push(this.onErr);
                },
                complete: () => {
                    this.loadingCatalog = false;
                }
            });
        }
        else {
            this.catalogService.getCatalogsBySystem(kat.id).subscribe({
                next: (res) => {
                    this.catalog = res;
                    this.err = false;
                },
                error: (err) => {
                    this.infoPopup.showErrorDownload("katalogu");
                    this.err = true;
                    this.catalog.push(this.onErr);
                },
                complete: () => {
                    this.loadingCatalog = false;
                }
            });
        }
    }
    showDialogToAddCatalog() {
        const dialogRef = this.dialog.open(_catalog_add_form_catalog_add_form_component__WEBPACK_IMPORTED_MODULE_7__["CatalogAddFormComponent"], {
            minWidth: '800px',
            panelClass: 'no-padding-dialog'
        });
        dialogRef.afterClosed().subscribe(result => {
            this.loadData();
            console.log('The dialog catalog add was closed');
        });
    }
    showDialogToAddSystem() {
        const dialogRef = this.dialog.open(_system_add_form_system_add_form_component__WEBPACK_IMPORTED_MODULE_6__["SystemAddFormComponent"], {
            width: '450px',
            panelClass: 'no-padding-dialog'
        });
        dialogRef.afterClosed().subscribe(result => {
            this.loadData();
            console.log('The dialog system add was closed');
        });
    }
    onRowEditInit(catalog) {
        this.clonedCatalogs[catalog.id] = Object.assign({}, catalog);
    }
    onRowEditSave(catalog) {
        this.catalogService.updateCatalog(catalog.id, catalog).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("katalogu");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("katalogu");
            },
            complete: () => {
                delete this.clonedCatalogs[catalog.id];
            }
        });
    }
    onRowDeleteCatalog(katalog) {
        this.confirmationService.confirm({
            message: 'Na pewno chcesz usunąć ' + katalog.number + '?',
            acceptLabel: 'Tak',
            rejectLabel: 'Nie',
            accept: () => {
                //Actual logic to perform a confirmation
                this.catalogService.deleteCatalog(katalog.id).subscribe({
                    next: (res) => {
                        this.infoPopup.showSuccessDelete("katalogu");
                    },
                    error: (err) => {
                        this.infoPopup.showErrorDelete("katalogu");
                    },
                    complete: () => {
                        this.loadData();
                    }
                });
            }
        });
    }
    showBoolean(val) {
        if (val) {
            return "TAK";
        }
        else {
            return "";
        }
    }
    onRowEditCancel(catalog, index) {
        this.catalog[index] = this.clonedCatalogs[catalog.id];
        delete this.clonedCatalogs[catalog.id];
    }
    //============== SYSTEMS ==================
    onRowEditSystem(system) {
        this.clonedSystem[system.id] = Object.assign({}, system);
    }
    onRowEditSaveSystem(system) {
        this.catalogService.updateSystem(system.id, system).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("systemu");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("systemu");
            },
            complete: () => {
                delete this.clonedSystem[system.id];
            }
        });
    }
    onRowDeleteSystem(system) {
        this.confirmationService.confirm({
            message: 'Na pewno chcesz usunąć ' + system.name + '?',
            acceptLabel: 'Tak',
            rejectLabel: 'Nie',
            accept: () => {
                //Actual logic to perform a confirmation
                this.catalogService.deleteSystem(system.id).subscribe({
                    next: (res) => {
                        this.infoPopup.showSuccessDelete("systemu");
                    },
                    error: (err) => {
                        this.infoPopup.showErrorDelete("systemu");
                    },
                    complete: () => {
                        this.loadData();
                    }
                });
            }
        });
    }
    onRowEditCancelSystem(system, index) {
        this.system[index] = this.clonedSystem[system.id];
        delete this.clonedSystem[system.id];
    }
};

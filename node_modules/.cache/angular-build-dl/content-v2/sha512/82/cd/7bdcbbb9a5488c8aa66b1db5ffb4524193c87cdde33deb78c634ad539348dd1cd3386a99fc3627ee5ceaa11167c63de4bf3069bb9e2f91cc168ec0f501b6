let CatalogAddFormComponent = class CatalogAddFormComponent {
    constructor(dialogRef, catalogService, infoPopup) {
        this.dialogRef = dialogRef;
        this.catalogService = catalogService;
        this.infoPopup = infoPopup;
    }
    ngOnInit() {
        this.filteredSystems = this.catalogService.systems;
        this.formCatalog = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormGroup"]({
            number: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required] }),
            profileType: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required] }),
            systemProfil: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required] }),
            offerName: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimOuter: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimInner: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimOneSide: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimTwoSide: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimTwoSideInner: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            dimTwoSideOuter: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](''),
            actualMeasurement: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](false),
            hard: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](false),
            comments: new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]('')
        });
    }
    filterSystems(event) {
        this.filteredSystems = [];
        for (let i = 0; i < this.catalogService.systems.length; i++) {
            let sys = this.catalogService.systems[i];
            if (sys.name.toLowerCase().indexOf(event.query.toLowerCase()) == 0) {
                this.filteredSystems.push(sys);
            }
        }
    }
    onNoClick() {
        this.dialogRef.close();
    }
    onAddNewCatalog() {
        if (this.isString(this.formCatalog.value.systemProfil)) {
            let tmpSys = { id: null, name: this.formCatalog.value.systemProfil };
            this.formCatalog.value.systemProfil = tmpSys;
        }
        console.log(this.formCatalog.value);
        this.catalogService.addCatalog(this.formCatalog.value).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessAdd("katalogu");
                this.onNoClick();
            },
            error: (err) => {
                this.infoPopup.showErrorAdd("katalogu");
            },
            complete: () => {
            }
        });
    }
    isString(value) {
        return typeof value === 'string' || value instanceof String;
    }
};

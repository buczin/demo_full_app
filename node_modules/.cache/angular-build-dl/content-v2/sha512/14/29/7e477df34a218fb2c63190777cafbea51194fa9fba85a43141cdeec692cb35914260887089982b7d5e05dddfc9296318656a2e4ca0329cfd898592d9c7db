        this.selectedFilm = event.target.value;
    }
    showSuccessNew() {
        this.messageService.add({ severity: 'success', summary: 'Dodanie Okleiny', detail: 'Poprawnie Dodano Pozycję' });
    }
    showError() {
        this.messageService.add({ severity: 'error', summary: 'ERROR', detail: 'Nie udało się dodać pozycji' });
    }

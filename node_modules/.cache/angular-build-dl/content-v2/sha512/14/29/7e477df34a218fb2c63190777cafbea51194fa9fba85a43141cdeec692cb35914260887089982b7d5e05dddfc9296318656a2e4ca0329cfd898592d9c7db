        this.messageService = messageService;
        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA },
        ];
        this.selectedFilm = this.available[0].value;
    }
    ngOnInit() {
        this.formFilm = new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormGroup"]({
            number: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required] }),
            name: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required] }),
            producer: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required] }),
            imgLink: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"](''),

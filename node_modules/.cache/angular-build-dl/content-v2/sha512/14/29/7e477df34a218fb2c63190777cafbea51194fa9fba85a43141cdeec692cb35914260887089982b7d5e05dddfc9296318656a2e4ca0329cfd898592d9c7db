            ralNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"](''),
            ecoPlastNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_4__["FormControl"]('')
        });
    }

let FilmMainComponent = class FilmMainComponent {
    constructor(filmService, messageService) {
        this.filmService = filmService;
        this.messageService = messageService;
        this.film = [];
        this.onErr = {
            id: 0,
            number: "---",
            name: "---",
            producer: "---",
            ecoPlastNumber: "---",
            ralNumber: "---",
            imgLink: "---",
            status: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA
        };
        this.clonedFilms = {};
    }
    ngOnInit() {
        this.err = false;
        this.loadData();
        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_2__["OkleinaEnum"].NIEDOSTEPNA },
        ];
    }
    loadData() {
        this.filmService.getOklieny().subscribe({
            next: (res) => {
                console.log(res);
                this.film = res;
                this.err = false;
            },
            error: (err) => {
                console.log(err);
                this.showErrorDownload();
                this.err = true;
                this.film.push(this.onErr);
            },
            complete: () => {
            }
        });
    }
    showDialogToAdd() {
        this.filmService.displayDialog = true;
    }
    showSuccess() {
        this.messageService.add({ severity: 'success', summary: 'Edycja Okleiny', detail: 'Poprawnie Zaktualizowano Pozycję' });
    }
    showError() {
        this.messageService.add({ severity: 'error', summary: 'ERROR', detail: 'Nie udało się zaktualizować pozycji' });
    }
    showErrorDownload() {
        this.messageService.add({ severity: 'error', summary: 'ERROR', detail: 'Nie udało się pobrać pozycji. Proszę odświeżyć stronę' });
    }
    clear() {
        this.messageService.clear();
    }
    onRowEditInit(film) {
        this.clonedFilms[film.id] = Object.assign({}, film);
    }
    onRowEditSave(film) {
        console.log(film);
        this.filmService.updateOkleina(film.id, film).subscribe({
            next: (res) => {
                console.log(res);
                this.showSuccess();
            },
            error: (err) => {
                console.log(err);
                this.showError();
            },
            complete: () => {
                delete this.clonedFilms[film.id];
                this.loadData();
            }
        });
        // if (film.numerOkleiny > 0) {
        //     delete this.clonedFilms[film.numerOkleiny];
        //    // this.messageService.add({severity:'success', summary: 'Success', detail:'Car is updated'});
        // }
        // else {
        //     //this.messageService.add({severity:'error', summary: 'Error', detail:'Year is required'});
        // }
    }
    onRowEditCancel(film, index) {
        this.film[index] = this.clonedFilms[film.id];
        delete this.clonedFilms[film.id];
    }
};
FilmMainComponent.ctorParameters = () => [

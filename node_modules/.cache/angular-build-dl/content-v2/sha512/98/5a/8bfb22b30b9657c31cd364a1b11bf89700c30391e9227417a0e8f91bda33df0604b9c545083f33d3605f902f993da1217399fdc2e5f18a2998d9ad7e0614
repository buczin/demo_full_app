class SettingsService {
    constructor(http) {
        this.http = http;
    }
    // =============== USERS ============
    getUsers() {
        return this.http.get("api/users");
    }
    addUser(data) {
        return this.http.post("api/user", data);
    }
    updateUser(id, data) {
        return this.http.put("api/user/" + id, data, { responseType: 'text' });
    }
    deleteUser(id) {
        return this.http.delete("api/user/" + id, { responseType: 'text' });
    }
    changePassword(id, data) {
        return this.http.put("api/user/pass/" + id, data);
    }
    //============== MY COMPANY ===============
    getMyCompanies() {
        return this.http.get("api/mycompanies");
    }
    getMyCompanyById(id) {
        return this.http.get("api/mycompany/" + id);
    }
    addMyCompany(data) {
        return this.http.post("api/mycompany", data);
    }
    updateMyCompany(id, data) {
        return this.http.put("api/mycompany/" + id, data, { responseType: 'text' });
    }
    deleteMyCompany(id) {
        return this.http.delete("api/mycompany/" + id, { responseType: 'text' });
    }
    getHelpers() {
        return this.http.get("api/helpers");
    }
    updateHelpersExchange(data) {
        return this.http.post("api/helpers/exchange", data);
    }
    updateHelpersSetUpCost(data) {
        return this.http.post("api/helpers/clientsetupcost", data);
    }
    updateHelpersSetCost(data) {
        return this.http.post("api/helpers/setcost", data);
    }
    updateHelpersColorChange(data) {
        return this.http.post("api/helpers/color-change", data);
    }
    updateHelpersCostsRange(data) {
        return this.http.put("api/helpers/costs-range", data);
    }
    // ============ PRINT OFFER TEXT =============
    updatePrintText(data) {
        return this.http.post("api/helpers/default-print-text", data);
    }
}

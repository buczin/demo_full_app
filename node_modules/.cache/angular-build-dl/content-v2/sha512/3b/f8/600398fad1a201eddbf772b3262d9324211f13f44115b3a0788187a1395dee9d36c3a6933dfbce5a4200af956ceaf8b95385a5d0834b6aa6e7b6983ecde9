class OfferResolver {
    constructor(offerService) {
        this.offerService = offerService;
    }
    resolve(route, state) {
        console.log("Resolve Offer with id: " + route.paramMap.get('id'));
        return this.offerService.getOfferById(route.paramMap.get('id'));
    }
}

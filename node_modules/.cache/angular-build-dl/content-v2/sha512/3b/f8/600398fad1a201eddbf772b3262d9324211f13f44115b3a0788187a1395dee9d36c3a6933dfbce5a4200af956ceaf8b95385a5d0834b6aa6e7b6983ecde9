const _c0 = function () { return [10, 20, 50]; };
class MainOffersComponent {
    constructor(dialogService, offerService, infoPopup, globalService) {
        this.dialogService = dialogService;
        this.offerService = offerService;
        this.infoPopup = infoPopup;
        this.globalService = globalService;
        this.offersCount = 0;
        this.loadingTable = false;
        this.optionsSort = [];
    }
    ngOnInit() {
        this.optionsSort = [
            { label: "Wszystkie", value: '-1' },
            { label: "Nowe", value: _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].NEW },
            { label: "W przygotowaniu", value: _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].INPREPARATION },
            { label: "Wysłana", value: _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].SENT },
            { label: "Zaakceptowana", value: _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].ACCEPTED },
            { label: "Anulowana", value: _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].CANCELED },
        ];
        this.loadData();
        //this.offerService.getAllOrdersNew();
    }
    showDialogToAddOffer() {
        const ref = this.dialogService.open(_form_new_offer_form_new_offer_component__WEBPACK_IMPORTED_MODULE_1__["FormNewOfferComponent"], {
            header: 'Nowa Oferta',
            styleClass: 'no-padding-dialog',
            width: '500px',
        });
        ref.onClose.subscribe(result => {
            console.log('The dialog add new offer was closed');
        });
    }
    loadData() {
        this.globalService.loadingGlobal = true;
        this.loadingTable = true;
        this.offerService.getCountOffers().subscribe({
            next: (res) => {
                this.offersCount = res;
            },
            error: (err) => {
                this.offersCount = 0;
                this.infoPopup.showErrorDownload("ilości zleceń");
            },
            complete: () => { }
        });
        this.offerService.getAllOffers().subscribe({
            next: (res) => {
                this.allOffers = [];
                this.allOffers = res;
                this.offersNew = this.allOffers.filter(x => x.offerStatus == _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].NEW);
                this.offersInpreparation = this.allOffers.filter(x => x.offerStatus == _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].INPREPARATION);
                this.offersSent = this.allOffers.filter(x => x.offerStatus == _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].SENT);
                this.offersAccepted = this.allOffers.filter(x => x.offerStatus == _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].ACCEPTED);
                this.offersCanceled = this.allOffers.filter(x => x.offerStatus == _offers_service__WEBPACK_IMPORTED_MODULE_2__["OfferStatus"].CANCELED);
                this.loadingTable = false;
            },
            error: (err) => {
                this.infoPopup.showErrorDownload("ofert");
            },
            complete: () => {
                this.globalService.loadingGlobal = false;
            }
        });
    }
    loadByStatus(value) {
        if (value != '-1') {
            this.offerService.getAllOffersByStatus(value).subscribe({
                next: (res) => {
                    this.allOffers = [];
                    this.allOffers = res;
                },
                error: (err) => {
                    this.infoPopup.showErrorDownload("ofert");
                },
                complete: () => { }
            });
        }
        else {
            this.loadData();
        }
    }
    // SORT FUNCTION
    customSort(event) {
        event.data.sort((data1, data2) => {
            let value1 = data1[event.field];
            let value2 = data2[event.field];
            let result = null;
            let result2 = null;
            if (value1 == null && value2 != null)
                result = -1;
            else if (value1 != null && value2 == null)
                result = 1;
            else if (value1 == null && value2 == null)
                result = 0;
            else if (typeof value1 === 'string' && typeof value2 === 'string' && event.field != "offerNumber")
                result = value1.localeCompare(value2);
            else if (event.field == "offerNumber") {
                if (Math.abs(value1.substring(value1.length - 4)) < Math.abs(value2.substring(value2.length - 4)))
                    return (event.order * -1);
                if (Math.abs(value1.substring(value1.length - 4)) > Math.abs(value2.substring(value2.length - 4)))
                    return (event.order * 1);
                if (Math.abs(value1.substring(0, value1.length - 4)) < Math.abs(value2.substring(0, value2.length - 4)))
                    return (event.order * -1);
                if (Math.abs(value1.substring(0, value1.length - 4)) > Math.abs(value2.substring(0, value2.length - 4)))
                    return (event.order * 1);
            }
            else
                result = (value1 < value2) ? -1 : (value1 > value2) ? 1 : 0;
            return (event.order * result);
        });
    }
}

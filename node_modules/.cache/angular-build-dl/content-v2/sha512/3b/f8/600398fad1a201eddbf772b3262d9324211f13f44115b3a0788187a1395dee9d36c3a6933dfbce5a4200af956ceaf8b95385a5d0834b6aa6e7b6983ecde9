class UpdateOfferPositionComponent {
    constructor(productionService, settingService, offerService, cookieService, infoPopup, ref, config) {
        this.productionService = productionService;
        this.settingService = settingService;
        this.offerService = offerService;
        this.cookieService = cookieService;
        this.infoPopup = infoPopup;
        this.ref = ref;
        this.config = config;
        this.helper = {
            exchangeRateAlu: null,
            exchangeRatePcv: null,
            defaultSetCost: null,
            defaultClientSetUpCost: null,
            defaultPrintTextHeader: null,
            defaultPrintTextTableFotter: null,
            defaultCostChangeColor: null
        };
        this.selectedSystem = null;
        this.selectedCatalog = null;
        this.searchBy = ['number'];
        this.sideSug = ["zew", "wew", "jedno", "środek", "obu", "obu-zew", "obu-wew"];
        this.loading = false;
        this.changeSetUpCost = false;
        this.offerData = config.data[0];
        this.offerPosition = config.data[1];
    }
    ngOnInit() {
        this.findSystem(this.offerPosition.catalogIdHelper);
        this.filterBySystem();
        this.settingService.getHelpers().subscribe({
            next: (res) => {
                this.helper = res[0];
            },
            error: (err) => {
                console.log(err);
            },
            complete: () => {
            }
        });
        this.formOfferPosition = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            profilType: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required] }),
            profilNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required] }),
            alu: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null),
            catalogIdHelper: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            warranty: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null),
            offerPositionSides: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormArray"]([
                new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
                    id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    side: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    filmNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    filmWidth: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
                    filmEntrusted: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    quantityMb: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
                    hard: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    changeColor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](false),
                    costSetupAdditional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    exchangeRate: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    bicolor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    costChangeColor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
                    additional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
                    cost: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
                    mIs: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
                })
            ])
        });
        this.loadStructure().then(x => {
            this.formOfferPosition.patchValue(this.offerPosition);
        });
    }
    loadStructure() {
        return new Promise((resolve, reject) => {
            for (let index = 1; index < this.offerPosition.offerPositionSides.length; index++) {
                this.addSide();
            }
            resolve();
        });
    }
    findSystem(id) {
        if (id != null && id != '') {
            let cat = this.productionService.catalogSuggestions.find(x => x.id == id);
            this.selectedSystem = cat.systemProfil;
            this.selectedCatalog = cat;
        }
    }
    filterBySystem() {
        this.allCatalog = [];
        if (this.selectedSystem != null) {
            for (let i = 0; i < this.productionService.catalogSuggestions.length; i++) {
                let cat = this.productionService.catalogSuggestions[i];
                if (cat.systemProfil.name != null) {
                    if (cat.systemProfil.id === (this.selectedSystem.id)) {
                        this.allCatalog.push(cat);
                    }
                }
            }
            this.cookieService.set('creatorSelectedSystemOffer', JSON.stringify(this.selectedSystem));
        }
        else {
            this.allCatalog = this.productionService.catalogSuggestions;
        }
    }
    searchCatalog(event) {
        this.catalogSugFiltered = [];
        if (event.query != "") {
            console.log("loading");
            for (let i = 0; i < this.allCatalog.length; i++) {
                let cat = this.allCatalog[i];
                if (cat.number != null && this.searchBy.includes("number")) {
                    if (cat.number.toLowerCase().includes(event.query.toLowerCase())) {
                        this.catalogSugFiltered.push(cat);
                    }
                }
                if (cat.profileType != null && this.searchBy.includes("name")) {
                    if (cat.profileType.toLowerCase().includes(event.query.toLowerCase())) {
                        this.catalogSugFiltered.push(cat);
                    }
                }
            }
        }
        else {
            this.allCatalog.forEach(x => this.catalogSugFiltered.push(x));
        }
    }
    duplicateLastSide() {
        let tmp = this.sides.at(this.sides.length - 1);
        tmp.get('id').patchValue(null);
        this.addSide();
        this.sides.at(this.sides.length - 1).patchValue(tmp.value);
    }
    searchFilm(event) {
        this.filmSugFiltered = [];
        for (let i = 0; i < this.productionService.filmSuggestions.length; i++) {
            let film = this.productionService.filmSuggestions[i];
            if (film.number.toLowerCase().includes(event.query.toLowerCase()) || film.name.toLowerCase().includes(event.query.toLowerCase())) {
                this.filmSugFiltered.push(film);
            }
        }
    }
    searchSide(event) {
        this.sideSugFiltered = [];
        for (let i = 0; i < this.sideSug.length; i++) {
            let side = this.sideSug[i];
            if (side.toLowerCase().indexOf(event.query.toLowerCase()) == 0) {
                this.sideSugFiltered.push(side);
            }
        }
    }
    exit(id) {
        this.ref.close(id);
    }
    get sides() {
        return this.formOfferPosition.get("offerPositionSides");
    }
    addSide() {
        const group = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            side: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            filmNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null),
            filmWidth: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
            filmEntrusted: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](false),
            quantityMb: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](6.5, { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
            hard: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](false),
            changeColor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](false),
            costSetupAdditional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            bicolor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null),
            costChangeColor: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null),
            exchangeRate: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null, { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
            additional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](this.offerData.client.helper_additional, { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
            cost: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](this.offerData.client.helper_cost, { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
            mIs: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](null, { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].nullValidator] }),
        });
        this.sides.push(group);
        this.changeHard(this.sides.length - 1);
        if (this.formOfferPosition.get('alu').value) {
            this.sides.controls[this.sides.length - 1]['controls']['exchangeRate'].setValue(this.helper.exchangeRateAlu);
        }
        else {
            this.sides.controls[this.sides.length - 1]['controls']['exchangeRate'].setValue(this.helper.exchangeRatePcv);
        }
    }
    removeSide(i) {
        this.sides.removeAt(i);
    }
    onSelectFilm(event, i) {
        this.sides.controls[i].get('filmNumber').setValue(event.number);
        this.sides.controls[i].get('bicolor').setValue(event.surchargePercentage);
    }
    onSelectSide(e, index) {
        if (this.selectedCatalog != null) {
            let res = "";
            switch (e) {
                case "jedno":
                    res = this.selectedCatalog.dimOneSide;
                    break;
                case "środek":
                    res = this.selectedCatalog.dimCenterSide;
                    break;
                case "obu":
                    res = this.selectedCatalog.dimTwoSide;
                    break;
                case "obu-zew":
                    res = this.selectedCatalog.dimTwoSideOuter;
                    break;
                case "obu-wew":
                    res = this.selectedCatalog.dimTwoSideInner;
                    break;
                case "zew":
                    res = this.selectedCatalog.dimOuter;
                    break;
                case "wew":
                    res = this.selectedCatalog.dimInner;
                    break;
            }
            this.sides.controls[index].get('hard').setValue(this.selectedCatalog.hard);
            this.sides.controls[index].get('filmWidth').setValue(res);
        }
    }
    onSelectCatalog(event) {
        this.selectedCatalog = event;
        this.formOfferPosition.get("profilNumber").setValue(event.number);
        this.formOfferPosition.get("profilType").setValue(event.offerName);
        this.formOfferPosition.get("alu").setValue(event.alu);
        this.formOfferPosition.get("catalogIdHelper").setValue(event.id);
        this.sides.controls.forEach((x, index) => {
            let e = x.get('side').value;
            let res = "";
            switch (e) {
                case "jedno":
                    res = this.selectedCatalog.dimOneSide;
                    break;
                case "środek":
                    res = this.selectedCatalog.dimCenterSide;
                    break;
                case "obu":
                    res = this.selectedCatalog.dimTwoSide;
                    break;
                case "obu-zew":
                    res = this.selectedCatalog.dimTwoSideOuter;
                    break;
                case "obu-wew":
                    res = this.selectedCatalog.dimTwoSideInner;
                    break;
                case "zew":
                    res = this.selectedCatalog.dimOuter;
                    break;
                case "wew":
                    res = this.selectedCatalog.dimInner;
                    break;
            }
            this.sides.controls[index].get('hard').setValue(this.selectedCatalog.hard);
            this.sides.controls[index].get('filmWidth').setValue(res);
        });
    }
    calculateSides() {
        for (let index = 0; index < this.sides.length; index++) {
            this.changeHard(index);
            // if(this.sides.controls[index]['controls']['costChangeColor'].value){
            //   this.sides.controls[index]['controls']['colorChangeBool'].setValue(true);
            // }
        }
    }
    colorFieldChange(i, event) {
        if (event) {
            this.sides.controls[i]['controls']['changeColor'].setValue(true);
        }
        else {
            this.sides.controls[i]['controls']['changeColor'].setValue(false);
        }
    }
    changeColorChange(i, event) {
        if (event) {
            this.sides.controls[i]['controls']['costChangeColor'].setValue(this.offerData.client.helper_costChangeColor);
        }
        else {
            this.sides.controls[i]['controls']['costChangeColor'].setValue('');
        }
    }
    changeHard(i) {
        let event = this.sides.controls[i]['controls']['quantityMb'].value;
        this.loadM(event, i);
    }
    changeAlu(event) {
        if (event) {
            this.sides.controls.forEach(x => {
                x['controls']['exchangeRate'].setValue(this.helper.exchangeRateAlu);
            });
        }
        else {
            this.sides.controls.forEach(x => {
                x['controls']['exchangeRate'].setValue(this.helper.exchangeRatePcv);
            });
        }
    }
    loadM(event, i) {
        let tmpHard = this.sides.controls[i]['controls']['hard'].value;
        if (event <= 50) {
            if (tmpHard) {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mHardTo50);
            }
            else {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mEasyTo50);
            }
        }
        else if (event > 50 && event <= 150) {
            if (tmpHard) {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mHardTo150);
            }
            else {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mEasyTo150);
            }
        }
        else if (event > 150 && event <= 500) {
            if (tmpHard) {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mHardTo500);
            }
            else {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mEasyTo500);
            }
        }
        else if (event > 500) {
            if (tmpHard) {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mHardAbove500);
            }
            else {
                this.sides.controls[i]['controls']['mIs'].setValue(this.offerData.client.helper_mEasyAbove500);
            }
        }
    }
    updateOfferPosition() {
        this.loading = true;
        this.offerService.updateOfferPosition(this.offerData.offerNumber, this.offerPosition.id, this.formOfferPosition.value).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessAdd("pozycja");
                this.loading = false;
                this.exit(1);
            },
            error: (err) => {
                this.infoPopup.showErrorAdd("pozycja");
                this.loading = false;
                console.log(err);
            }
        });
    }
}

                  } else {
                    _focusCell3.tabIndex = '0';

                    _focusCell3.focus();
                  }
                } else {
                  this.navigateToMonth(false, groupIndex);
                }

                event.preventDefault();
                break;
              }
            //enter

            case 13:
              {
                this.onDateSelect(event, date);
                event.preventDefault();
                break;
              }
            //escape

            case 27:
              {
                this.overlayVisible = false;
                event.preventDefault();
                break;
              }
            //tab

            case 9:
              {
                if (!this.inline) {
                  this.trapFocus(event);
                }

                break;
              }


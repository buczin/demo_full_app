 m-0\">\n                                    <button pButton pRipple type=\"button\" (click)=\"removeSide(i)\"\n                                        matTooltip=\"Usuń stronę\" matTooltipClass=\"tooltip-red\" icon=\"pi pi-times\"\n                                        class=\"p-button-raised p-button-danger p-button-sm\"></button>\n                                </div>\n                                <div class=\"col\">\n                                    <span class=\"text-center pt-1\">{{i + 1}}</span>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"p-0 pl-1\">\n                            <label class=\"sb-small-label labelRow\" for=\"side\">Strona kształtownika</label>\n                            <p-autoComplete [inputStyle]=\"{'font-weight':'bold'}\" formControlName=\"side\" [suggestions]=\"sideSugFiltered\"\n                                (completeMethod)=\"searchSide($event)\" [dropdown]=\"true\"\n                                (onSelect)=\"onSelectSide($event,i)\">\n                                <ng-template let-side pTemplate=\"item\">\n                                    <div class=\"ui-helper-clearfix padding-item-list\"\n                                        style=\"border-bottom:1px solid #D5D5D5\">\n                                        <div style=\"font-size:16px;\">{{side}}</div>\n                                    </div>\n                                </ng-template>\n                            </p-autoComplete>\n                        </div>\n                        <div class=\"p-0 pl-3\">\n                            <label class=\"sb-small-label labelRow\" for=\"filmNumber\">Numer Okleiny</label>\n                            <p-autoComplete [inputStyle]=\"{'font-weight':'bold'}\" formControlName=\"filmNumber\" [minLength]=\"1\"\n                                [suggestions]=\"filmSugFiltered\" (completeMethod)=\"searchFilm($event
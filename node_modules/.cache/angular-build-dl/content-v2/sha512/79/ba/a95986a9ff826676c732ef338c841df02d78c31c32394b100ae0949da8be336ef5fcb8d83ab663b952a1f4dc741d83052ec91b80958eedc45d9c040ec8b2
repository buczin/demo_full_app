   </div>\n    </div>\n</div>","import { Component, OnInit, Inject } from '@angular/core';\nimport { MAT_DIALOG_DATA, MatDialogRef } from '@angular/material/dialog';\nimport { ProductionServiceService } from '../../production-service/production-service.service';\nimport { InfoPopupComponent } from 'src/app/info-popup/info-popup.component';\nimport { FormGroup, FormControl, FormArray } from '@angular/forms';\nimport { combineAll } from 'rxjs/operators';\nimport { ConfirmationService } from 'primeng/api';\n\n@Component({\n  selector: 'app-form-add-update-used-film',\n  templateUrl: './form-add-update-used-film.component.html',\n  styleUrls: ['./form-add-update-used-film.component.scss']\n})\nexport class FormAddUpdateUsedFilmComponent implements OnInit {\n\n  formUsedFilm: FormGroup;\n  orderNumber: any;\n  iUseTmp: any;\n\n  constructor(\n    @Inject(MAT_DIALOG_DATA) public orderData: any,\n    public dialogRef: MatDialogRef<FormAddUpdateUsedFilmComponent>,\n    private productionService: ProductionServiceService,\n    private infoPopup: InfoPopupComponent,\n    private confirmationService: ConfirmationService\n  ) {\n\n    this.formUsedFilm = new FormGroup({\n      postionSide: new FormArray([new FormGroup({\n        id: new FormControl(''),\n        side: new FormControl(''),\n        usedFilm: new FormArray([\n          new FormGroup({\n            id: new FormControl(''),\n            width: new FormControl(''),\n            length: new FormControl('')\n          })])\n      })\n      ])\n    });\n  }\n\n  ngOnInit() {\n    this.orderNumber = this.orderData.productionOrder.orderNumber;\n    this.loadData();\n  }\n\n  loadData() {\n    this.orderData.postionSide.forEach((el, index) => {\n      if (index != 0) {\n        this.addPostionSide();\n      }\n      el.usedFilm.forEach((u, dindex) => {\n        if (dindex != 0) {\n          this.addUsedFilm(index);\n        }\n      });\n    });\n    this.formUsedFilm.patchValue(this.orderData);\n  }\n\n  // SIDE\n  get postionSide(): FormArray {\n    return this.formUsedFilm.get(\"postionSide\") as FormArray\n  }\n\n  addPostionSide() {\n    this.postionSide.push(new FormGroup({\n      id: new FormControl(''),\n      side: new FormControl(''),\n      usedFilm: new FormArray([\n        new FormGroup({\n          id: new FormControl(''),\n          width: new FormControl(''),\n          length: new FormControl('')\n        })])\n    }));\n  }\n\n  removePostionSide(i: number) {\n    this.postionSide.removeAt(i);\n  }\n\n  //USE\n  get usedFilm(): FormArray {\n    return this.postionSide.controls[this.iUseTmp].get('usedFilm') as FormArray\n  }\n\n  addUsedFilm(row) {\n    this.iUseTmp = row;\n    this.usedFilm.push(new FormGroup({\n      id: new FormControl(''),\n      width: new FormControl(''),\n      length: new FormControl('')\n    }));\n  }\n\n  removeUsedFilm(row, index) {\n    this.iUseTmp = row;\n    if (this.usedFilm.value[index].id != \"\") {\n      this.confirmationService.confirm({\n        message: 'Na pewno chcesz usun pozycj zuycia?',\n        header: 'Potwierdzenie',\n        acceptLabel: 'Tak',\n        rejectLabel: 'Nie',\n        acceptIcon: 'pi pi-trash',\n        acceptButtonStyleClass: 'p-button-raised p-button-sm p-button-danger',\n        rejectButtonStyleClass: 'p-button-raised p-button-outlined p-button-secondary p-button-sm',\n        accept: () => {\n          this.usedFilm.removeAt(index);\n        }\n      });\n    } else {\n      this.usedFilm.removeAt(index);\n    }\n  }\n\n  addOrderPositionSideUsedFilm() {\n    console.log(this.formUsedFilm.value);\n    this.productionService.addOrderPositionSideUsedFilm(this.orderNumber, this.orderData.id, this.formUsedFilm.value).subscribe(\n      {\n        next: (res) => {\n          console.log(res)\n          this.exit();\n        },\n        error: (err) => { console.log(err) },\n        complete: () => { }\n      })\n  }\n\n  exit() {\n    this.dialogRef.close();\n  }\n\n}\n","<div class=\"sb-dialog-titlebar\"
1);\n  }\n\n  ngOnInit(): void {\n\n  }\n\n\n  getNotes() {\n    this.offerService.getOfferNotes(this.offerNumber).subscribe({\n      next: (res) => {\n        this.offerNotes = res;\n        this.offerNotes.sort((a, b) => {\n          if (a.createdDate > b.createdDate) {\n            return 1;\n          }\n          if (a.createdDate < b.createdDate) {\n            return -1;\n          }\n          return 0;\n        });\n      },\n      error: (err) => {\n        console.log(err);\n      },\n      complete: () => {\n      }\n    })\n  }\n\n  addNewNote(offerNumber, newNote) {\n    this.offerService.addNewOfferNote(offerNumber, newNote).subscribe({\n      next: (res) => {\n        this.infoPopup.showSuccessAdd('notatk');\n        this.newNote = \"\";\n        this.getNotes();\n      },\n      error: (err) => {\n        console.log(err);\n      },\n      complete: () => {\n      }\n    })\n  }\n\n  deleteNote(offerNumber, id) {\n    this.offerService.deleteOfferNote(offerNumber, id).subscribe({\n      next: (res) => {\n        this.infoPopup.showSuccessDelete('notatk');\n        this.getNotes();\n      },\n      error: (err) => {\n        console.log(err);\n      },\n      complete: () => {\n      }\n    })\n  }\n\n}\n","<div class=\"sb-timeline-widget\">\n    <!-- <div class=\"timeline-line\"></div> -->\n    <ul class=\"sb-timeline\">\n        <li *ngFor=\"let item of offerNotes\">\n            <div class=\"date\">\n                <span>{{item.createdDate | date:'dd.MM.yyyy, HH:mm'}}</span>\n                <i class=\"pi pi-comment\" style=\"font-size: 2rem\"></i>\n            </div>\n            <div class=\"message\">\n                <div class=\"checked\" [innerHTML]=\"item.message\"></div>\n                <button pButton pRipple type=\"button\" (click)=\"deleteNote(offerNumber,item.id)\" pTooltip=\"Usu notatke\" icon=\"pi pi-trash\"\n                class=\"p-button-sm sb-button red\"></button>\n            </div>\n            <div class=\"break\"></div>\n
class FilmMainComponent {
    constructor(dialog, auth, filmService, infoPopup, confirmationService) {
        this.dialog = dialog;
        this.auth = auth;
        this.filmService = filmService;
        this.infoPopup = infoPopup;
        this.confirmationService = confirmationService;
        this.film = [];
        this.producer = [];
        this.positionpersite = 10;
        this.onErr = {
            id: 0,
            number: "---",
            name: "---",
            producer: "---",
            ecoPlastNumber: "---",
            ralNumber: "---",
            imgLink: "---",
            status: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].NIEDOSTEPNA
        };
        this.clonedFilms = {};
        this.clonedProducers = {};
        this.loading = false;
    }
    ngOnInit() {
        this.err = false;
        this.loadData();
        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].NIEDOSTEPNA },
            { label: 'Dost. Ograniczona', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].DOSTEPNOSCOGRANICZONA },
        ];
    }
    loadData() {
        this.loading = true;
        this.displayDialogOkleina = false;
        this.filmService.loadProducersSelect();
        this.filmService.getOklieny().subscribe({
            next: (res) => {
                this.film = res;
                this.err = false;
            },
            error: (err) => {
                this.infoPopup.showErrorDownload("okleiny");
                this.err = true;
                this.film.push(this.onErr);
            },
            complete: () => {
                this.loading = false;
            }
        });
        this.filmService.getCountOkleina().subscribe({
            next: (res) => {
                this.filmCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("oklein");
            },
            complete: () => {
            }
        });
        this.filmService.getCountProducer().subscribe({
            next: (res) => {
                this.producerCount = res;
            },
            error: (err) => {
                this.infoPopup.showErrorCount("producentów");
            },
            complete: () => {
            }
        });
    }
    onDialogHide() {
        this.displayDialogOkleina = false;
        this.selectedOkleina = null;
    }
    showDialogFilm(data) {
        this.displayDialogOkleina = true;
        this.selectedOkleina = data;
    }
    showDialogToAddFilm() {
        const dialogRef = this.dialog.open(_film_add_form_film_add_form_component__WEBPACK_IMPORTED_MODULE_2__["FilmAddFormComponent"], {
            width: '550px',
            panelClass: 'no-padding-dialog'
        });
        dialogRef.afterClosed().subscribe(result => {
            this.loadData();
            console.log('The dialog add production order was closed');
        });
    }
    showDialogToAddProducer() {
        const dialogRef = this.dialog.open(_producer_add_form_producer_add_form_component__WEBPACK_IMPORTED_MODULE_3__["ProducerAddFormComponent"], {
            width: '450px',
            panelClass: 'no-padding-dialog'
        });
        dialogRef.afterClosed().subscribe(result => {
            this.loadData();
            console.log('The dialog add production order was closed');
        });
    }
    onRowEditInit(film) {
        this.clonedFilms[film.id] = Object.assign({}, film);
    }
    onRowEditSave(film) {
        this.filmService.updateOkleina(film.id, film).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("okleiny");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("okleiny");
            },
            complete: () => {
                delete this.clonedFilms[film.id];
                this.loadData();
            }
        });
    }
    onRowDeleteFilm(film) {
        this.confirmationService.confirm({
            message: 'Na pewno chcesz usunąć ' + film.name + '?',
            acceptLabel: 'Tak',
            rejectLabel: 'Nie',
            acceptIcon: 'pi pi-trash',
            acceptButtonStyleClass: 'p-button-raised p-button-sm p-button-danger',
            rejectButtonStyleClass: 'p-button-raised p-button-outlined p-button-secondary p-button-sm',
            accept: () => {
                this.filmService.deleteOklina(film.id).subscribe({
                    next: (res) => {
                        this.infoPopup.showSuccessDelete("okleiny");
                    },
                    error: (err) => {
                        console.log(err);
                        this.infoPopup.showErrorDelete("okleiny");
                    },
                    complete: () => {
                        this.loadData();
                    }
                });
            }
        });
    }
    onRowEditCancel(film, index) {
        this.film[index] = this.clonedFilms[film.id];
        delete this.clonedFilms[film.id];
    }
    onRowEditInitProducer(producer) {
        this.clonedProducers[producer.id] = Object.assign({}, producer);
    }
    onRowEditProducer(producer) {
        this.filmService.updateProducer(producer.id, producer).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessUpdate("producenta");
            },
            error: (err) => {
                this.infoPopup.showErrorUpdate("producenta");
            },
            complete: () => {
                delete this.clonedProducers[producer.id];
                this.loadData();
            }
        });
    }
    onRowDeleteProducer(producer) {
        this.confirmationService.confirm({
            message: 'Na pewno chcesz usunąć ' + producer.producerName + '?',
            acceptLabel: 'Tak',
            rejectLabel: 'Nie',
            acceptIcon: 'pi pi-trash',
            acceptButtonStyleClass: 'p-button-raised p-button-sm p-button-danger',
            rejectButtonStyleClass: 'p-button-raised p-button-outlined p-button-secondary p-button-sm',
            accept: () => {
                this.filmService.deleteProducer(producer.id).subscribe({
                    next: (res) => {
                        this.infoPopup.showSuccessDelete("producent");
                    },
                    error: (err) => {
                        this.infoPopup.showErrorDelete("producent");
                    },
                    complete: () => {
                        this.loadData();
                    }
                });
            }
        });
    }
    onRowEditCancelProducer(producer, index) {
        this.producer[index] = this.clonedFilms[producer.id];
        delete this.clonedProducers[producer.id];
    }
}

class FilmAddFormComponent {
    constructor(dialogRef, filmService, infoPopup) {
        this.dialogRef = dialogRef;
        this.filmService = filmService;
        this.infoPopup = infoPopup;
        this.available = [
            { label: 'Dostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].DOSTEPNA },
            { label: 'Niedostępna', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].NIEDOSTEPNA },
            { label: 'Dost. Ograniczona', value: _film_service_films_service__WEBPACK_IMPORTED_MODULE_1__["OkleinaEnum"].DOSTEPNOSCOGRANICZONA },
        ];
    }
    ngOnInit() {
        this.formFilm = new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormGroup"]({
            number: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required], asyncValidators: [src_app_validators_AppValidators__WEBPACK_IMPORTED_MODULE_3__["AppValidators"].filmExists], updateOn: 'blur' }),
            name: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required] }),
            producer: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required] }),
            imgLink: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](''),
            status: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](this.available[0], { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required] }),
            ralNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](''),
            ecoPlastNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"]('')
        });
    }
    selectFilm(event) {
        this.selectedFilm = event.target.value;
    }
    exit() {
        this.dialogRef.close();
    }
    addNewFilm() {
        this.formFilm.value.status = this.formFilm.value.status.value;
        this.filmService.addOklina(this.formFilm.value).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessAdd("okleiny");
                this.exit();
            },
            error: (err) => {
                this.infoPopup.showErrorAdd("okleiny");
            },
            complete: () => {
            }
        });
    }

class ProducerAddFormComponent {
    constructor(dialogRef, filmService, infoPopup) {
        this.dialogRef = dialogRef;
        this.filmService = filmService;
        this.infoPopup = infoPopup;
    }
    ngOnInit() {
        this.formProducer = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            producerName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', { validators: [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required], asyncValidators: [_validators_AppValidators__WEBPACK_IMPORTED_MODULE_2__["AppValidators"].producerExists], updateOn: 'blur' }),
        });
    }
    exit() {
        this.dialogRef.close();
    }
    addNewProducer() {
        this.filmService.addProducer(this.formProducer.value).subscribe({
            next: (res) => {
                this.infoPopup.showSuccessAdd("producenta");
                ;
                this.exit();
            },
            error: (err) => {
                this.infoPopup.showErrorAdd("producenta");
            },
            complete: () => {
            }
        });
    }

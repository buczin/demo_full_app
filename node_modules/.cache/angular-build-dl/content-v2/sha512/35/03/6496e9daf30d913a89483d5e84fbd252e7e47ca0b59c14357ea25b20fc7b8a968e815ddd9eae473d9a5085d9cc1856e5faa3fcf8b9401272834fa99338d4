var OkleinaEnum;
(function (OkleinaEnum) {
    OkleinaEnum["DOSTEPNA"] = "DOSTEPNA";
    OkleinaEnum["NIEDOSTEPNA"] = "NIEDOSTEPNA";
})(OkleinaEnum || (OkleinaEnum = {}));
let FilmsService = class FilmsService {
    constructor(http) {
        this.http = http;
        this.producers = [];
        this.producersSelect = [];
    }
    loadProducersSelect() {
        console.log("Load producers in service");
        this.producersSelect = [];
        this.getProducers().subscribe({
            next: (res) => {
                res.forEach(element => {
                    this.producers = res;
                    let el = { label: element.producerName, value: element };
                    this.producersSelect.push(el);
                });
            },
            error: (err) => {
            },
            complete: () => {
            }
        });
    }
    getOklieny() {
        return this.http.get("api/films");
    }
    getOklina(id) {
        return this.http.get("api/film/" + id);
    }
    getCountOkleina() {
        return this.http.get("api/film/count");
    }
    addOklina(data) {
        return this.http.post("api/film", data);
    }
    updateOkleina(id, data) {
        return this.http.put("api/film/" + id, data);
    }
    deleteOklina(id) {
        return this.http.delete("api/film/" + id, { responseType: 'text' });
    }
    // PRODUCERS
    getProducers() {
        return this.http.get("api/film/producers");
    }
    getProducer(id) {
        return this.http.get("api/film/producer/" + id);
    }
    getCountProducer() {
        return this.http.get("api/film/producer/count");
    }
    addProducer(data) {
        return this.http.post("api/film/producer", data);
    }
    updateProducer(id, data) {
        return this.http.put("api/film/producer/" + id, data);
    }
    deleteProducer(id) {
        return this.http.delete("api/film/producer/" + id, { responseType: 'text' });
    }
};
FilmsService.ctorParameters = () => [
    { type: _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpClient"] }
];
